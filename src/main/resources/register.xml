<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="default3" extends="struts-default">
		<action name="addUser" class="userAction" method="add" >
			<result type="redirect">/listUser</result>
		</action>
		
		<action name="listUser" method="list" class="userAction">
			<result name="success">/view/register.jsp</result>
		</action>
	</package>
</struts>